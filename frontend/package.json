{
  "name": "champions-league-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docker": "vite build --mode docker",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,vue}\" \"__tests__/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.{js,vue}\" \"__tests__/**/*.js\""
  },
  "dependencies": {
    "axios": "^1.13.2",
    "vue": "^3.5.25",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-vue": "^10.6.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.4.7",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
